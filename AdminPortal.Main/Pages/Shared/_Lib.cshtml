@using TKW.AdminPortal.Enums
@model ImportLibViewModel
<script>
    @if (!String.IsNullOrEmpty(Model.Js)) {
     <text>   if (!$("script[src='@Model.Js']").length) {
            var Js = document.createElement("script");
            Js.src = "@Model.Js";
                $("head").append(Js)
                    }
    </text>}
    @if (!String.IsNullOrEmpty(Model.Css))
    {<text>
        if (!$("link[href='@Model.Css']").length) {
            var Css = document.createElement("link");
            Css.rel = "stylesheet";
            Css.href = "@Model.Css";
            $("head").append(Css);
    }
    </text>}
</script>
