                                                <li><a class="dropdown-item" href="../Pages/Ajax/Address/Modal/Edit">Set as Default</a></li>
                                            </ul>
                                        </div>
                                    </div>


                                    <div class="mt-3 d-flex align-items-center">
                                        <div class="flex-fill">
                                            @if (s.AddressLine != null)
                                            {
                                                <address class="m-b-none d-inline" type="Shop" name="@s.Name" mobile-no="@s.MobileNo" addressline="@s.AddressLine" locality="@s.LocalityName" state="@s.StateName" city="@s.CityName" pincode="@s.Pincode"></address>
                                            }
                                            else
