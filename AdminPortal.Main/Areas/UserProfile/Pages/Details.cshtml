@page
@model TKW.AdminPortal.Areas.UserProfile.Pages.DetailsModel
@using Newtonsoft.Json
@{
    ViewBag.Title = "UserProfile";

}

<pagetitle title="User Details" description="All details of the User.">
</pagetitle>
<div class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body profile-user-box">
                    <div class="d-flex align-items-center">
                        <div class="flex-fill">
                            <div class="row align-items-center">
                                <div class="col-auto">
                                    <div>
                                        <img src="@Model.User.PictureName" alt="" class="rounded-circle img-thumbnail" style="height:6rem; width:6rem;">
                                    </div>
                                </div>
                                <div class="col ms-1">
                                    <div>
                                        <h4>@Model.User.Name</h4>
                                        <ul class="mb-0 mt-3 list-inline">
                                            <li class="list-inline-item me-5">
                                                <div class="small font-trans text-muted pb-1">MOBILE NUMBER</div>
                                                <span>@Model.User.MobileNo</span>
                                            </li>
                                            <li class="list-inline-item me-5">
                                                <div class="small font-trans text-muted pb-1">DATE OF BIRTH</div>
                                                <span>8 Nov 1998</span>
                                            </li>
                                            <li class="list-inline-item me-5">
                                                <div class="small font-trans text-muted pb-1">GENDER</div>
                                                <span>Male</span>
                                            </li>
                                            <li class="list-inline-item me-5">
                                                <div class="small font-trans text-muted pb-1">JOINING DATE</div>
                                                <span>27 Sep 2018</span>
                                            </li>
                                            <li class="list-inline-item me-5">
                                                <div class="small font-trans text-muted pb-1">EMAIL</div>
                                                @if (Model.User.Email != null)
                                                {
                                                    <span>@Model.User.Email</span>
                                                }
                                                else
                                                {
                                                    <span>N/A</span>
                                                }
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="me-3">
                            <button type="button" class="btn btn-primary btn-outline btn-sm">
                                Edit Profile
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-3">
                    <div class="card mb-3">
                        <h6 class="header-title ms-3 mt-3">Payment Details</h6>
                        <div class="card-body px-0">
                            <div class="ps-3 d-flex align-items-center">
                                    <div class="flex-fill">
                                        <div class="small font-trans text-muted mb-2"><strong>DEFAULT PAYMENT METHOD</strong></div>
                                        @if (Model.User.DefaultPaymentMethodId != null)
                                        {
                                            <span>@Model.User.DefaultPaymentMethodId</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </div>
                                    <button class="btn btn-outline btn-xs float-end me-3" data-bs-target="#EditDefaultPaymentModal" data-bs-toggle="modal" type="button" data-load-url="@Url.Page("Ajax/Modal/EditDefaultPayment",new {Model.User.Id})">
                                        <i class="bi bi-pencil-square fs-6"></i>
                                    </button>
                                </div>

                            <hr />

                            <div class="ps-3 d-flex align-items-center">
                                <div class="flex-fill">
                                    <div class="small font-trans text-muted mb-2"><strong>UPI ADDRESS</strong> </div>
                                    @if (Model.User.UPIAddress != null)
                                    {
                                        <span>@Model.User.UPIAddress</span>
                                    }
                                    else
                                    {
                                        <span>N/A</span>
                                    }
                                </div>
                                <button class="btn btn-outline btn-xs float-end me-3" data-bs-target="#EditUpiAddressModal" data-bs-toggle="modal" type="button" data-load-url="@Url.Page("Ajax/Modal/EditUpiAddress",new {Model.User.Id})">
                                    <i class="bi bi-pencil-square fs-6"></i>
                                </button>
                            </div>

                            <div class="d-flex align-items-center ps-3 mt-4">
                                <div class="flex-fill">
                                    <div class="small font-trans text-muted mb-2"><strong>BANK ACCOUNT DETAILS</strong></div>
                                    <div>
                                        @if (Model.User.BankAccountNumber != null)
                                        {
                                            <span>@Model.User.BankAccountNumber</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }

                                        @if (Model.User.BankIFSC != null)
                                        {
                                            <span class="small text-muted mt-2 mb-2 ms-1">(IFSC:<span class="ms-1">@Model.User.BankIFSC)</span></span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </div>
                                </div>
                                <div>
                                    <button class="btn btn-outline btn-xs float-end me-3" data-bs-target="#EditBankAccountDetailsModal" data-bs-toggle="modal" type="button" data-load-url="@Url.Page("Ajax/Modal/EditBankAccountDetails",new {Model.User.Id, HandlerId  =  Model.User.Id})">
                                        <i class="bi bi-pencil-square fs-6"></i>
                                    </button>
                                </div>
                            </div>

