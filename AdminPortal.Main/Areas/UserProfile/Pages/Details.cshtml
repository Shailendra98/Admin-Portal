                                            {
                                                <em>Address not found..!</em>
                                            }
                                        </div>
                                        <div>
                                            <button class="btn btn-outline btn-xs me-3 float-end" type="button" data-bs-toggle="modal" data-bs-target="#EditModal" data-load-url="@Url.Page("../Pages/Ajax/Address/Modal/Edit", new {s.Id})">
                                                <i class="bi bi-pencil-square fs-6"></i>
                                            </button>
                                        </div>
                                    </div>
                                }
                            </div>

